{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server/index.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/server/index.ts"
    }
  ],
  "functions": {
    "server/index.ts": {
      "maxDuration": 30,
      "memory": 1024
    }
  }
}
